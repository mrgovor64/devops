node {
    withCredentials([usernamePassword(credentialsId: 'someuser', usernameVariable: 'USERNAME', passwordVariable: 'PASSWORD')]) {
        password = env.PASSWORD
        user = env.USERNAME
    }
	sh '''cd edit\ data\ in\ config\ from\ jenkins\ credentials'''
    sh "python includeCredentials.py someconfig.conf --parameters=\"user='${user}' password='${password}'\" "
}